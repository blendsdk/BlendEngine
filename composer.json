{
    "minimum-stability": "dev",
    "name": "blendsdk/blendengine",
    "description": "Generic Backoffice Application Framework",
    "keywords": [],
    "homepage": "https://github.com/blendsdk/BlendEngine",
    "license": "MIT",
    "authors": [
        {
            "name": "Gevik Babakhani",
            "email": "gevik@blendjs.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/event-dispatcher": "~2.3,<3.0",
        "symfony/http-foundation": "~2.3,<3.0",
        "symfony/http-kernel": "~2.3,<3.0",
        "symfony/routing": "~2.3,<3.0",
        "monolog/monolog": "^1.15@dev",
        "twig/twig": "^1.19"
    },
    "autoload": {
        "psr-4": {
            "Blend\\": "src/Blend"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blend\\Tests\\": "tests/Blend/Tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
